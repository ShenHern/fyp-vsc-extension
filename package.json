{
  "name": "unettrace",
  "displayName": "UnetTrace",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.1"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ShenHern/fyp-vsc-extension"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unettrace.solve",
        "title": "UnetTrace: Align trace.json Timings of 2 Nodes"
      },
      {
        "command": "unettrace.trace",
        "title": "UnetTrace: Create Visualisation from Trace File"
      },
      {
        "command": "unettrace.combine",
        "title": "UnetTrace: Combine Aligned trace.json Files"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jquery": "^3.5.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.73.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "esbuild": "^0.17.11",
    "eslint": "^8.29.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/gaussian": "^1.2.0",
    "@types/object-hash": "^3.0.2",
    "@types/stream-json": "^1.7.2",
    "gaussian": "^1.3.0",
    "node-fetch": "^3.3.0",
    "object-hash": "^3.0.0",
    "stream-json": "^1.7.5",
    "ts-gaussian": "^3.0.2"
  }
}
