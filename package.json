{
  "name": "unettrace",
  "version": "0.0.1",
  "displayName": "Unet Trace",
  "description": "",
  "engines": {
    "vscode": "^1.71.0"
  },
  "publisher": "Unet",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:unettrace.visual",
    "onCommand:unettrace.solve"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unettrace.visual",
        "title": "UnetTrace: Create Visualisation from Trace File",
        "icon": {
          "light": "./assets/unet.png",
          "dark": "./assets/unet.png"
        }
      },
      {
        "command": "unettrace.solve",
        "title": "UnetTrace: Combine & Aligned Trace Files"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "unettrace.visual",
          "when": "resourceExtname == .json || editorLangId == json",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "start": "react-scripts start",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "node ./out/test/runTest.js",
    "build": "node ./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "watch-build": "nodemon --watch src --ext js,tsx,ts --exec \"yarn run build\"",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/gaussian": "^1.2.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/stream-json": "^1.7.3",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "console-browserify": "^1.2.0",
    "constants-browserify": "^1.0.0",
    "crypto-browserify": "^3.12.0",
    "eslint": "^8.36.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "react-scripts": "^5.0.1",
    "rewire": "^6.0.0",
    "stream-browserify": "^3.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url": "^0.11.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@projectstorm/react-diagrams": "^6.7.4",
    "@types/jsonstream": "^0.8.31",
    "@types/react-cytoscapejs": "^1.2.2",
    "@types/vscode-webview": "^1.57.1",
    "big-json": "^3.2.0",
    "cytoscape": "^3.23.0",
    "fs": "^0.0.1-security",
    "gaussian": "^1.3.0",
    "JSONStream": "^1.3.5",
    "object-hash": "^3.0.0",
    "react": "^18.2.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.0",
    "react-icons": "^4.4.0",
    "react-linkify-it": "^1.0.7",
    "react-side-pane": "^2.4.6",
    "styled-components": "^5.3.6",
    "terser": "^5.15.0",
    "yarn": "^1.22.19",
    "zustand": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
