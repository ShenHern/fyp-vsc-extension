{
  "name": "unettrace",
  "displayName": "UnetTrace",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "unettrace.helloWorld",
        "title": "UnetTrace: Hello World!"
      },
      {
        "command": "unettrace.solve",
        "title": "UnetTrace: Solve Tx-Rx"
      },
      {
        "command": "unettrace.trace",
        "title": "UnetTrace: Create Visualisation from Trace File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/jquery": "^3.5.14",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.73.1",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.29.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@types/object-hash": "^3.0.2",
    "@types/stream-json": "^1.7.2",
    "node-fetch": "^3.3.0",
    "object-hash": "^3.0.0",
    "stream-json": "^1.7.5",
    "ts-gaussian": "^3.0.2"
  }
}
